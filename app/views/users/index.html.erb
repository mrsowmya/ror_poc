<table>
	<thead class="thead-dark">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Address</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>

  	<% @users.each do |user| %>
	    <tr>
	    	<td><%= user.first_name %></td>
	    	<td><%= user.last_name %></td>
	    	<td><%= user.email %></td>
	    	<td><%= user.phone_number %></td>
	    	<td><%= user.address %></td>
	    	<td><%= link_to "Edit", edit_user_path(user) %></td>
	    </tr>
	   <% end %>
  </tbody>

</table>


<br/>

<%= link_to "New User", new_user_path %>